<p id="notice"><%= notice %></p>

<h1>Txes</h1>

<table>
  <thead>
    <tr>
      <th>Txid</th>
      <th>Version</th>
      <th>Locktime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @txes.each do |tx| %>
      <tr>
        <td><%= tx.txid %></td>
        <td><%= tx.version %></td>
        <td><%= tx.locktime %></td>
        <td><%= link_to 'Show', tx %></td>
        <td><%= link_to 'Edit', edit_tx_path(tx) %></td>
        <td><%= link_to 'Destroy', tx, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tx', new_tx_path %>
